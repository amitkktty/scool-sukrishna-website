RewriteEngine on 

# Force non-www to www
RewriteCond %{HTTP_HOST} ^www\.(.+)$ [NC]
RewriteRule ^ https://%1%{REQUEST_URI} [L,R=301]

# Force HTTPS
RewriteCond %{HTTPS} off
RewriteCond %{HTTP:X-Forwarded-SSL} !on
RewriteCond %{HTTP_HOST} ^sukrishnacommerce\.com$
RewriteRule ^(.*)$ https://sukrishnacommerce.com/$1 [R=301,L]



RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME}\.php -f
RewriteRule ^(.*)$ $1.php [NC,L]
Redirect /ca /ca-in-patna.php
# RewriteRule /best_commerce_coaching_in_patna_for_11_and_12.php /board-courses.php [NC,L]




